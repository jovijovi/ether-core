{
  "name": "@jovijovi/ether-core",
  "version": "0.4.0",
  "description": "Out-of-the-box core APIs for Ethereum ecosystem",
  "author": "jovijovi <mageyul@hotmail.com>",
  "license": "MIT",
  "keywords": [
    "typescript",
    "blockchain",
    "ether-core",
    "ethereum",
    "ether",
    "core",
    "API",
    "Out-of-the-box"
  ],
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jovijovi/ether-core.git"
  },
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "test": "jest --coverage --forceExit",
    "build": "yarn clean && tsc --build",
    "clean-pub": "rm -rf ./publish/* && mkdir -p ./publish",
    "prepub": "yarn clean-pub && yarn build && cp -ap ./dist/lib ./publish/lib && cp -ap LICENSE ./publish/ && cp -ap package.json ./publish/ && cp -ap README.md ./publish/"
  },
  "dependencies": {
    "@jovijovi/ether-network": "^0.2.2",
    "@jovijovi/pedrojs-common": "^1.1.12",
    "ethers": "5.6.9"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@typechain/ethers-v5": "10.1.0",
    "@types/jest": "28.1.6",
    "@types/node": "18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "jest": "28.1.3",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typechain": "8.1.0",
    "typescript": "4.7.4",
    "yaml": "^2.1.1"
  }
}
